<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="robots" content="index,follow">
  <title>{{ data.title }} | {{ metadata.title }}</title>
  <link rel="icon" href="{{ metadata.siteUrl }}/static/images/favicon.png" type="image/png">
  {% if summary %}
    <meta name="description" content="{{ summary | safe }}">
  {% endif %}
  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
</head>
